<?xml version="1.0" encoding="UTF-8"?>
<Configuration status = "WARN">
    <Appenders>
        <Console name = "Console" target = "SYSTEM_OUT">
            <PatternLayout pattern = "%d{HH:mm:ss.SS} [%t] %-5level %logger{36} - %msg%n" />
        </Console>
        <File name = "ProgramFile" filename = "D:\ДИАНА\ПРОЕКТ\library\log\StateProgram.log" immediateFlush = "false" append = "false">
            <PatternLayout pattern = "%d{yyy-MM-dd HH:mm:ss.SS} [%t] %-5level %logger{36} - %msg%n" />
        </File>
    </Appenders>
    <Loggers>
        <Root level = "debug">
            <AppenderRef ref = "Console"/>
            <AppenderRef ref = "ProgramFile"/>
        </Root>
    </Loggers>
</Configuration>
